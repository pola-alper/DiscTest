                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:textColor="@android:color/black"
                android:textSize="20sp" />

            <TextView
                style="@style/c_result"
                android:layout_width="wrap_content"
