<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="70dip" />

            <gradient android:angle="-90" android:endColor="#263238" android:startColor="#263238"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="70dip" />

            <solid android:color="#263238"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="70dip" />

            <gradient android:angle="-90" android:endColor="#000000" android:startColor="#000000" />
        </shape>
    </item>
</selector>