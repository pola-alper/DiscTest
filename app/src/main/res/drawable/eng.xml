<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="70dip" />

            <gradient android:angle="-90" android:endColor="#795548" android:startColor="#795548"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="70dip" />

            <solid android:color="#795548"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="70dip" />

            <gradient android:angle="-90" android:endColor="#3E2723" android:startColor="#3E2723" />
        </shape>
    </item>
</selector>